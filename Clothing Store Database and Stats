--Create a clothing store database

CREATE TABLE clothing (id INTEGER PRIMARY KEY, name text,quantity INTEGER, price INTEGER);

INSERT INTO clothing VALUES (1,"shorts",5,10);
INSERT INTO clothing VALUES (2, "shoes", 2,30);
INSERT INTO clothing VALUES (3,"jeans",6,50);
INSERT INTO clothing VALUES (4,"dress",5, 35);
INSERT INTO clothing VALUES (5,"jackets",8, 45);

--display the database ordered by price.
SELECT * FROM clothing 
ORDER BY price ASC;

--What is the sum quantity of clothing?
SELECT SUM(quantity)FROM clothing
